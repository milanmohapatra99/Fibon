    "def f(n):\n",
    "    if n== 0:\n",
    "        return 0\n",
    "    elif n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return f(n-1)+f(n-2)\n",
    "    \n",
    "for i in range(0,12):\n",
    "    print(f(i))\n",
    "    "
